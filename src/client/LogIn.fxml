<BorderPane prefHeight="400.0" prefWidth="600.0" styleClass="LogIn" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WPGUI.LogInController">
	<top>
		<Text strokeType="OUTSIDE" strokeWidth="0.0" text="LogIn" textAlignment="CENTER" wrappingWidth="195.50927734375" BorderPane.alignment="CENTER">
			<font>
				<Font size="53.0" />
			</font>
		</Text>
	</top>
	<left>
		<VBox alignment="CENTER_RIGHT" prefHeight="382.0" prefWidth="23.0" spacing="35.0" BorderPane.alignment="CENTER">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="ID">
					<font>
						<Font size="39.0" />
					</font>
				</Text>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="PWD">
					<font>
						<Font size="39.0" />
					</font>
				</Text>
			</children>
		</VBox>
	</left>
	<center>
		<VBox alignment="CENTER" fillWidth="false" prefHeight="382.0" prefWidth="377.0" spacing="20.0" BorderPane.alignment="CENTER">
			<children>
				<TextField fx:id="txtID" prefHeight="39.0" prefWidth="264.0">
					<font>
						<Font size="30.0" />
					</font>
				</TextField>
				<PasswordField fx:id="txtPWD" prefHeight="47.0" prefWidth="263.0">
					<font>
						<Font size="30.0" />
					</font>
				</PasswordField>
			</children>
		</VBox>
	</center>
	<right>
		<Button fx:id="btnLogin" mnemonicParsing="false" onAction="#handleBtnLoginAction" prefHeight="90.0" prefWidth="113.0" text="LogIn" BorderPane.alignment="CENTER">
			<font>
				<Font size="27.0" />
			</font>
		</Button>
	</right>
	<opaqueInsets>
		<Insets />
	</opaqueInsets>
	<padding>
		<Insets left="50.0" right="50.0" top="50.0" />
	</padding>
   <bottom>
      <Text fx:id="txtLoginFail" fill="#f54343" text="" BorderPane.alignment="CENTER">
